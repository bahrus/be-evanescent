{
  "name": "be-evanescent",
  "version": "0.0.0",
  "description": "be-evanescent is a web component decorator that removes the element it adorns from the DOM tree when the web component definitions it monitors for become registered.",
  "main": "be-evanescent.js",
  "module": "be-evanescent.js",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "FOUC",
    "transient-style"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
    "package-check": "package-check"
  },
  "dependencies": {
    "be-decorated": "0.0.43",
    "be-hive": "0.0.9"
  },
  "devDependencies": {
    "xtal-test": "0.0.52",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.5.7"
  },
  "files":[
    "*.*"
  ],
  "exports":{
    ".": "./be-evanescent.js",
    "./be-evanescent.js": "./be-evanescent.js"
  },
  "types": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-evanescent.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-evanescent/issues"
  },
  "homepage": "https://github.com/bahrus/be-evanescent#readme"
}
